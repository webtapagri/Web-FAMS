<VirtualHost *:80>
    ServerName ams-dev1.tap-agri.com
    ServerAlias www.ams-dev1.tap-agri.com

    DocumentRoot /var/www/html/Web-FAMS/public

    <Directory "/var/www/html/">
        AllowOverride all
        Require all granted
    </Directory>

    ErrorLog ${APACHE_LOG_DIR}/error.log
    CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>
